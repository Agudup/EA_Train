      "AllocationPercent" ,
      "CapRatePercent" ,
      "EndDate" ,
      "FloorRatePercent" ,
      "InceptionIndexLevel" ,
      "ParticipationPercent" ,
      "ShieldRatePercent" ,
      "StartDate" ,
      "StepRatePercent" ,
      "StepRateSelectedIndicator" ,
      "SourceSystemID" ,
      "ChangeTimestampInSourceSystem" ,
      "ChangingUserInSourceSystem" ,
