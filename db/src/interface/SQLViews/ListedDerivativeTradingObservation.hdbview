view "sap.fsdm.SQLViews::ListedDerivativeTradingObservation_View" 
as select
      "MaturityDate" ,
      "PriceDataProvider" ,
      "PriceSeriesType" ,
      "PutOrCall" ,
      "SequenceNumber" ,
      "StrikePrice" ,
      "Timestamp" ,
      "_Exchange.MarketIdentifierCode" ,
      "_FinancialInstrument.FinancialInstrumentID" ,
      "BusinessValidFrom" ,
      "BusinessValidTo" ,
      "SystemValidFrom" ,
      "SystemValidTo" ,
      "_CommodityReferencePrice.ReferencePriceID" ,
      "Currency" ,
      "ListedDerivativeSymbol" ,
      "ListedDerivativeTradingObservationCategory" ,
      "Price" ,
      "PriceNotationForm" ,
      "Quantity" ,
      "StrikePriceCurrency" ,
      "TickID" ,
      "Unit" ,
      "SourceSystemID" ,
      "ChangeTimestampInSourceSystem" ,
      "ChangingUserInSourceSystem" ,
      "ChangingProcessType" ,
      "ChangingProcessID" 
  
from "sap.fsdm::ListedDerivativeTradingObservation"
with associations
(
	join "sap.fsdm.SQLViews::CommodityReferencePrice_View" as "_CommodityReferencePrice"
	  on "_CommodityReferencePrice"."ReferencePriceID" = "_CommodityReferencePrice.ReferencePriceID"
	     ,
	join "sap.fsdm.SQLViews::Exchange_View" as "_Exchange"
	  on "_Exchange"."MarketIdentifierCode" = "_Exchange.MarketIdentifierCode"
	     ,
	join "sap.fsdm.SQLViews::FinancialInstrument_View" as "_FinancialInstrument"
	  on "_FinancialInstrument"."FinancialInstrumentID" = "_FinancialInstrument.FinancialInstrumentID"
	     
);