                            order by "IN"."BusinessValidFrom") as "NX_",
                            lag ( "IN"."BusinessValidTo", 1)
                            over ( partition by
                                    "IN"."ASSOC_FeeSpecification.SequenceNumber",
                                    "IN"."ASSOC_FeeSpecification.ASSOC_FinancialContract.FinancialContractID",
                                    "IN"."ASSOC_FeeSpecification.ASSOC_FinancialContract.IDSystem",
                                    "IN"."ASSOC_FeeSpecification.ASSOC_TransferOrder.IDSystem",
                                    "IN"."ASSOC_FeeSpecification.ASSOC_TransferOrder.TransferOrderID",
                                    "IN"."ASSOC_FeeSpecification._Trade.IDSystem",
                                    "IN"."ASSOC_FeeSpecification._Trade.TradeID",
                                    "IN"."ASSOC_FeeSpecification._TrancheInSyndication.TrancheSequenceNumber",
                                    "IN"."ASSOC_FeeSpecification._TrancheInSyndication._SyndicationAgreement.FinancialContractID",
                                    "IN"."ASSOC_FeeSpecification._TrancheInSyndication._SyndicationAgreement.IDSystem",
