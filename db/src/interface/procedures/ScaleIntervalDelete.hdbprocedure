                        "IN"."ASSOC_StandardInterest.ASSOC_StandardProduct.FinancialStandardProductID",
                        "IN"."ASSOC_StandardInterest.ASSOC_StandardProduct.IDSystem",
                        "IN"."ASSOC_StandardInterest.ASSOC_StandardProduct.PricingScheme",
                        "IN"."ASSOC_StandardInterest.ASSOC_StandardProduct.ASSOC_Company.BusinessPartnerID",
                        "IN"."_PnLParticipation.SequenceNumber",
