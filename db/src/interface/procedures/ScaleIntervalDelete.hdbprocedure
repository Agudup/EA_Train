                        "IN"."ASSOC_StandardFee.ASSOC_StandardProduct.IDSystem",
                        "IN"."ASSOC_StandardFee.ASSOC_StandardProduct.PricingScheme",
                        "IN"."ASSOC_StandardFee.ASSOC_StandardProduct.ASSOC_Company.BusinessPartnerID",
