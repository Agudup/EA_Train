PROCEDURE "sap.fsdm.procedures::SubledgerAccountLoad" (IN ROW "sap.fsdm.tabletypes::SubledgerAccountTT")
  LANGUAGE SQLSCRIPT
  SQL SECURITY DEFINER
  AS
BEGIN

    --Check for period overlap
    declare period_overlap condition for sql_error_code 10001;
    declare exit handler for period_overlap
        begin
            declare err_msg clob;
            select TOP 1
                'Business Period Overlap: Key ' ||
                'SubledgerAccount=' || TO_VARCHAR("SubledgerAccount") || ' ' ||
                ':Business Period Overlap Error'
            into err_msg
            from
            (
                select
                    "IN"."BusinessValidFrom" as "X",
                    "IN"."BusinessValidTo" as "Y",
                    lag ( "IN"."BusinessValidFrom", 1)
                    over ( partition by
                        "IN"."SubledgerAccount"
                    order by "IN"."BusinessValidFrom") as "NX_",
                    lag("IN"."BusinessValidTo", 1)
                    over ( partition by
                        "IN"."SubledgerAccount"
                    order by "IN"."BusinessValidFrom") as "NY_",
                        "SubledgerAccount"
                from :row as "IN"
            )
            where ("X" >= "NX_" and "X" < "NY_") or ("Y" > "NX_" and "Y" <= "NY_");
            resignal set message_text = :err_msg;
        end;

    var_overlap = select *
                    from
                    (
                        select
                            "IN"."BusinessValidFrom" as "X",
                            "IN"."BusinessValidTo" as "Y",
                            lag ( "IN"."BusinessValidFrom", 1)
                            over ( partition by
                                    "IN"."SubledgerAccount"
                            order by "IN"."BusinessValidFrom") as "NX_",
                            lag ( "IN"."BusinessValidTo", 1)
                            over ( partition by
                                    "IN"."SubledgerAccount"
                            order by "IN"."BusinessValidFrom") as "NY_"
                from :row as "IN"
                )
                where ("X" >= "NX_" and "X" < "NY_") or ("Y" > "NX_" and "Y" <= "NY_");

    if not is_empty(:var_overlap) then
      signal period_overlap;
    end if;

    :var_overlap.delete();

    --Insert chunked versions of object
    insert into "sap.fsdm::SubledgerAccount" (
        "SubledgerAccount",
        "BusinessValidFrom",
        "BusinessValidTo",
        "_SubledgerAccountGroup.SubledgerAccountGroupId",
        "AssetLiabilityAssignment",
        "SubledgerAccountDescription",
        "SourceSystemID",
        "ChangeTimestampInSourceSystem",
        "ChangingUserInSourceSystem",
        "ChangingProcessType",
        "ChangingProcessID"
    )
    (
        select
            "OLD_SubledgerAccount" as "SubledgerAccount" ,
            "NX_" as "BusinessValidFrom" ,
            "BusinessValidFrom" as "BusinessValidTo" ,
            "OLD__SubledgerAccountGroup.SubledgerAccountGroupId" as "_SubledgerAccountGroup.SubledgerAccountGroupId" ,
            "OLD_AssetLiabilityAssignment" as "AssetLiabilityAssignment" ,
            "OLD_SubledgerAccountDescription" as "SubledgerAccountDescription" ,
            "OLD_SourceSystemID" as "SourceSystemID" ,
            "OLD_ChangeTimestampInSourceSystem" as "ChangeTimestampInSourceSystem" ,
            "OLD_ChangingUserInSourceSystem" as "ChangingUserInSourceSystem" ,
            "OLD_ChangingProcessType" as "ChangingProcessType" ,
            "OLD_ChangingProcessID" as "ChangingProcessID" 
        from
        (
            select
                lag("IN"."BusinessValidTo", 1, "OLD"."BusinessValidFrom")
                over ( partition by
                        "IN"."SubledgerAccount",
                        "OLD"."BusinessValidFrom"
                       order by "IN"."BusinessValidFrom") as "NX_",
                "IN"."BusinessValidFrom",
                                "OLD"."SubledgerAccount" as "OLD_SubledgerAccount",
                                "OLD"."BusinessValidFrom" as "OLD_BusinessValidFrom",
                                "OLD"."BusinessValidTo" as "OLD_BusinessValidTo",
                "OLD"."SystemValidFrom" as "OLD_SystemValidFrom",
                "OLD"."SystemValidTo" as "OLD_SystemValidTo",
                                "OLD"."_SubledgerAccountGroup.SubledgerAccountGroupId" as "OLD__SubledgerAccountGroup.SubledgerAccountGroupId",
                                "OLD"."AssetLiabilityAssignment" as "OLD_AssetLiabilityAssignment",
                                "OLD"."SubledgerAccountDescription" as "OLD_SubledgerAccountDescription",
                                "OLD"."SourceSystemID" as "OLD_SourceSystemID",
                                "OLD"."ChangeTimestampInSourceSystem" as "OLD_ChangeTimestampInSourceSystem",
                                "OLD"."ChangingUserInSourceSystem" as "OLD_ChangingUserInSourceSystem",
                                "OLD"."ChangingProcessType" as "OLD_ChangingProcessType",
                                "OLD"."ChangingProcessID" as "OLD_ChangingProcessID"
            from :row as "IN"
            inner join "sap.fsdm::SubledgerAccount" as "OLD"
            on
                ( case when "IN"."SubledgerAccount" is null then '' else "IN"."SubledgerAccount" end ) = "OLD"."SubledgerAccount" 
            where
                         ( "IN"."BusinessValidFrom" < "OLD"."BusinessValidTo" and "IN"."BusinessValidTo" > "OLD"."BusinessValidFrom" ) or
                         ( "IN"."BusinessValidFrom" = "OLD"."BusinessValidFrom" and "IN"."BusinessValidTo" = "OLD"."BusinessValidTo" ))
        where "BusinessValidFrom" > "NX_" );

    --Insert rightmost chunk
    insert into "sap.fsdm::SubledgerAccount" (
        "SubledgerAccount",
        "BusinessValidFrom",
        "BusinessValidTo",
        "_SubledgerAccountGroup.SubledgerAccountGroupId",
        "AssetLiabilityAssignment",
        "SubledgerAccountDescription",
        "SourceSystemID",
        "ChangeTimestampInSourceSystem",
        "ChangingUserInSourceSystem",
        "ChangingProcessType",
        "ChangingProcessID"
    )
    (
        select
            "OLD_SubledgerAccount" as "SubledgerAccount",
            "BusinessValidTo" as "BusinessValidFrom",
            "OLD_BusinessValidTo" as "BusinessValidTo",
            "OLD__SubledgerAccountGroup.SubledgerAccountGroupId" as "_SubledgerAccountGroup.SubledgerAccountGroupId",
            "OLD_AssetLiabilityAssignment" as "AssetLiabilityAssignment",
            "OLD_SubledgerAccountDescription" as "SubledgerAccountDescription",
            "OLD_SourceSystemID" as "SourceSystemID",
            "OLD_ChangeTimestampInSourceSystem" as "ChangeTimestampInSourceSystem",
            "OLD_ChangingUserInSourceSystem" as "ChangingUserInSourceSystem",
            "OLD_ChangingProcessType" as "ChangingProcessType",
            "OLD_ChangingProcessID" as "ChangingProcessID"
        from
        (
            select
                lead("IN"."BusinessValidFrom", 1, "OLD"."BusinessValidTo")
                over ( partition by
                        "IN"."SubledgerAccount",
                        "OLD"."BusinessValidFrom"
                order by "IN"."BusinessValidFrom") AS "NY_",
                "IN"."BusinessValidTo",
                        "OLD"."SubledgerAccount" as "OLD_SubledgerAccount",
                        "OLD"."BusinessValidFrom" as "OLD_BusinessValidFrom",
                        "OLD"."BusinessValidTo" as "OLD_BusinessValidTo",
            "OLD"."SystemValidFrom" as "OLD_SystemValidFrom",
            "OLD"."SystemValidTo" as "OLD_SystemValidTo",
                        "OLD"."_SubledgerAccountGroup.SubledgerAccountGroupId" as "OLD__SubledgerAccountGroup.SubledgerAccountGroupId",
                        "OLD"."AssetLiabilityAssignment" as "OLD_AssetLiabilityAssignment",
                        "OLD"."SubledgerAccountDescription" as "OLD_SubledgerAccountDescription",
                        "OLD"."SourceSystemID" as "OLD_SourceSystemID",
                        "OLD"."ChangeTimestampInSourceSystem" as "OLD_ChangeTimestampInSourceSystem",
                        "OLD"."ChangingUserInSourceSystem" as "OLD_ChangingUserInSourceSystem",
                        "OLD"."ChangingProcessType" as "OLD_ChangingProcessType",
                        "OLD"."ChangingProcessID" as "OLD_ChangingProcessID"
            from :row as "IN"
            inner join "sap.fsdm::SubledgerAccount" as "OLD"
            on
                ( case when "IN"."SubledgerAccount" is null then '' else "IN"."SubledgerAccount" end ) = "OLD"."SubledgerAccount" 
            where
                         ( "IN"."BusinessValidFrom" < "OLD"."BusinessValidTo" and "IN"."BusinessValidTo" > "OLD"."BusinessValidFrom" ) or
                         ( "IN"."BusinessValidFrom" = "OLD"."BusinessValidFrom" and "IN"."BusinessValidTo" = "OLD"."BusinessValidTo" ))
        where "NY_" = "OLD_BusinessValidTo" and "OLD_BusinessValidTo" > "BusinessValidTo");

    delete from "sap.fsdm::SubledgerAccount"
    where (
        "SubledgerAccount",
        "BusinessValidFrom",
        "BusinessValidTo"
    )
    in
    (
        select
            "OLD"."SubledgerAccount",
            "OLD"."BusinessValidFrom",
            "OLD"."BusinessValidTo"
        from :row as "IN"
        inner join "sap.fsdm::SubledgerAccount" as "OLD"
        on
           ( case when "IN"."SubledgerAccount" is null then '' else "IN"."SubledgerAccount" end ) = "OLD"."SubledgerAccount" 
        where
           ( "IN"."BusinessValidFrom" < "OLD"."BusinessValidTo" and "IN"."BusinessValidTo" > "OLD"."BusinessValidFrom" ) or
           ( "IN"."BusinessValidFrom" = "OLD"."BusinessValidFrom" and "IN"."BusinessValidTo" = "OLD"."BusinessValidTo" ));

    --Insert ALL the input data 
    insert into "sap.fsdm::SubledgerAccount" (
        "SubledgerAccount",
        "BusinessValidFrom",
        "BusinessValidTo",
        "_SubledgerAccountGroup.SubledgerAccountGroupId",
        "AssetLiabilityAssignment",
        "SubledgerAccountDescription",
        "SourceSystemID",
        "ChangeTimestampInSourceSystem",
        "ChangingUserInSourceSystem",
        "ChangingProcessType",
        "ChangingProcessID"
)    (
        select
            ( case when "SubledgerAccount" is null then '' else "SubledgerAccount" end ) as "SubledgerAccount",
            "BusinessValidFrom"  ,
            "BusinessValidTo"  ,
            "_SubledgerAccountGroup.SubledgerAccountGroupId"  ,
            "AssetLiabilityAssignment"  ,
            "SubledgerAccountDescription"  ,
            "SourceSystemID"  ,
            "ChangeTimestampInSourceSystem"  ,
            "ChangingUserInSourceSystem"  ,
            "ChangingProcessType"  ,
            "ChangingProcessID"  
        from :row );

END