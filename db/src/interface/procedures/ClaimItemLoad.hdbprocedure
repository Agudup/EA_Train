        where "NY_" = "OLD_BusinessValidTo" and "OLD_BusinessValidTo" > "BusinessValidTo");

    delete from "sap.fsdm::ClaimItem"
    where (
        "ID",
        "_Subclaim._Claimant._BusinessPartner.BusinessPartnerID",
