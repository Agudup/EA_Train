PROCEDURE "sap.fsdm.procedures::FinancialStatementDataDelReadOnly" (IN ROW "sap.fsdm.tabletypes::FinancialStatementDataTT_Del", OUT CURR_DEL "sap.fsdm.tabletypes::FinancialStatementDataTT_Del", OUT CURR_INS "sap.fsdm.tabletypes::FinancialStatementDataTT_Out")
