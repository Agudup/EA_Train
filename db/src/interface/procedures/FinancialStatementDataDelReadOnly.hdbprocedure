BEGIN

    --Check for period overlap
    declare period_overlap condition for sql_error_code 10001;
    declare exit handler for period_overlap
        begin
            declare err_msg clob;
            select TOP 1
                'Business Period Overlap: Key ' ||
                'ConsolidatedStatement=' || TO_VARCHAR("ConsolidatedStatement") || ' ' ||
                'FiscalPeriod=' || TO_VARCHAR("FiscalPeriod") || ' ' ||
                'ReportedAccountingSystem=' || TO_VARCHAR("ReportedAccountingSystem") || ' ' ||
                'StatementCurrency=' || TO_VARCHAR("StatementCurrency") || ' ' ||
                'ASSOC_BusinessPartner.BusinessPartnerID=' || TO_VARCHAR("ASSOC_BusinessPartner.BusinessPartnerID") || ' ' ||
                ':Business Period Overlap Error'
            into err_msg
            from
            (
                select
                    "IN"."BusinessValidFrom" as "X",
                    "IN"."BusinessValidTo" as "Y",
                    lag ( "IN"."BusinessValidFrom", 1)
                    over ( partition by
                        "IN"."ConsolidatedStatement",
                        "IN"."FiscalPeriod",
                        "IN"."ReportedAccountingSystem",
                        "IN"."StatementCurrency",
                        "IN"."ASSOC_BusinessPartner.BusinessPartnerID"
                    order by "IN"."BusinessValidFrom") as "NX_",
                    lag("IN"."BusinessValidTo", 1)
                    over ( partition by
                        "IN"."ConsolidatedStatement",
                        "IN"."FiscalPeriod",
                        "IN"."ReportedAccountingSystem",
                        "IN"."StatementCurrency",
                        "IN"."ASSOC_BusinessPartner.BusinessPartnerID"
                    order by "IN"."BusinessValidFrom") as "NY_",
                        "ConsolidatedStatement",
                        "FiscalPeriod",
                        "ReportedAccountingSystem",
                        "StatementCurrency",
                        "ASSOC_BusinessPartner.BusinessPartnerID"
                from :row as "IN"
            )
            where ("X" >= "NX_" and "X" < "NY_") or ("Y" > "NX_" and "Y" <= "NY_");
            resignal set message_text = :err_msg;
        end;

    var_overlap = select *
                    from
                    (
                        select
                            "IN"."BusinessValidFrom" as "X",
                            "IN"."BusinessValidTo" as "Y",
                            lag ( "IN"."BusinessValidFrom", 1)
                            over ( partition by
                                    "IN"."ConsolidatedStatement",
                                    "IN"."FiscalPeriod",
                                    "IN"."ReportedAccountingSystem",
                                    "IN"."StatementCurrency",
                                    "IN"."ASSOC_BusinessPartner.BusinessPartnerID"
                            order by "IN"."BusinessValidFrom") as "NX_",
                            lag ( "IN"."BusinessValidTo", 1)
                            over ( partition by
                                    "IN"."ConsolidatedStatement",
                                    "IN"."FiscalPeriod",
                                    "IN"."ReportedAccountingSystem",
                                    "IN"."StatementCurrency",
                                    "IN"."ASSOC_BusinessPartner.BusinessPartnerID"
                            order by "IN"."BusinessValidFrom") as "NY_"
                from :row as "IN"
                )
                where ("X" >= "NX_" and "X" < "NY_") or ("Y" > "NX_" and "Y" <= "NY_");

    if not is_empty(:var_overlap) then
      signal period_overlap;
    end if;

    :var_overlap.delete();
    CURR_DEL = select 
            "ConsolidatedStatement",
            "FiscalPeriod",
            "ReportedAccountingSystem",
            "StatementCurrency",
            "ASSOC_BusinessPartner.BusinessPartnerID",
            "BusinessValidFrom",
            "BusinessValidTo"
        from "sap.fsdm::FinancialStatementData" WHERE
            (
            "ConsolidatedStatement" ,
            "FiscalPeriod" ,
            "ReportedAccountingSystem" ,
            "StatementCurrency" ,
            "ASSOC_BusinessPartner.BusinessPartnerID" ,
            "BusinessValidFrom" ,
            "BusinessValidTo" 
            
)
