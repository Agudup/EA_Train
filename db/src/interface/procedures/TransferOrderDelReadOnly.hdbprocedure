                              ( "IN"."ItemNumber" = "OLD"."ItemNumber" or "IN"."ItemNumber" is null ) and
                              ( "IN"."TransferOrderID" = "OLD"."TransferOrderID" or "IN"."TransferOrderID" is null ) 
