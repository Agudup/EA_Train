PROCEDURE "sap.fsdm.procedures::TransferOrderDelReadOnly" (IN ROW "sap.fsdm.tabletypes::TransferOrderTT_Del", OUT CURR_DEL "sap.fsdm.tabletypes::TransferOrderTT_Del", OUT CURR_INS "sap.fsdm.tabletypes::TransferOrderTT_Out")
