PROCEDURE "sap.fsdm.procedures::FinancialContractStatementEraseReadOnly" (IN ROW "sap.fsdm.tabletypes::FinancialContractStatementTT_Erase", OUT CURR_DEL "sap.fsdm.tabletypes::FinancialContractStatementTT_Del" , OUT HIST_DEL "sap.fsdm.tabletypes::FinancialContractStatementTT_Del" )
  LANGUAGE SQLSCRIPT
  SQL SECURITY DEFINER
  READS SQL DATA
  AS
BEGIN

  declare semantic_keys_null_in_one_or_more_rows condition for sql_error_code 10001;
  declare exit handler for semantic_keys_null_in_one_or_more_rows
    BEGIN
      declare err_msg clob;
      select 'The input tabletype has one or more rows with all semantic keys as null values' into err_msg
        from "sap.fsdm.synonyms::DUMMY";
      resignal set message_text = :err_msg;
    END;
  var_find_null = select 1 as find_null
                    from :row
                   where
                         "StatementID" is null and
                         "_FinancialContract.FinancialContractID" is null and
                         "_FinancialContract.IDSystem" is null and
                         "_PositionCurrencyOfMultiCurrencyContract.PositionCurrency" is null and
                         "_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID" is null and
                         "_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem" is null 
;
  IF NOT IS_EMPTY(:var_find_null) THEN
    SIGNAL semantic_keys_null_in_one_or_more_rows;
  END IF;

--delete data from current table
CURR_DEL = select 
"StatementID",
"_FinancialContract.FinancialContractID",
"_FinancialContract.IDSystem",
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency",
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID",
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem",
"BusinessValidFrom",
"BusinessValidTo"
from "sap.fsdm::FinancialContractStatement" WHERE("StatementID" ,
"_FinancialContract.FinancialContractID" ,
"_FinancialContract.IDSystem" ,
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem" 
)in ( select "StatementID" ,
"_FinancialContract.FinancialContractID" ,
"_FinancialContract.IDSystem" ,
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem" 
from :ROW );

--delete data from history table
HIST_DEL = select 
"StatementID",
"_FinancialContract.FinancialContractID",
"_FinancialContract.IDSystem",
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency",
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID",
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem",
"BusinessValidFrom",
"BusinessValidTo"
from "sap.fsdm::FinancialContractStatement_Historical" WHERE("StatementID" ,
"_FinancialContract.FinancialContractID" ,
"_FinancialContract.IDSystem" ,
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem" 
)in ( select "StatementID" ,
"_FinancialContract.FinancialContractID" ,
"_FinancialContract.IDSystem" ,
"_PositionCurrencyOfMultiCurrencyContract.PositionCurrency" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.FinancialContractID" ,
"_PositionCurrencyOfMultiCurrencyContract.ASSOC_MultiCcyAccnt.IDSystem" 
from :ROW );

END
